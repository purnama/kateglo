<h2>Belum tergabung dengan Kateglo?</h2>
<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST">
    <fieldset>
        {{ form_widget(form) }}

        <button class="btn btn-large btn-primary btn-signup"
                type="submit">{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}</button>
    </fieldset>
</form>
