<h2>Ganti kata kunci</h2>
<form action="{{ path('fos_user_change_password') }}" {{ form_enctype(form) }} method="POST">
    <fieldset>
        {{ form_widget(form) }}

        <button class="btn btn-large btn-primary btn-signup"
                type="submit">{{ 'change_password.submit'|trans({}, 'FOSUserBundle') }}</button>

    </fieldset>
</form>
