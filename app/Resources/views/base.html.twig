<head>
    <meta charset="UTF-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <meta name="description" content="Kamus, Tesaurus dan Glosarium bahasa Indonesia."/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <link rel="shortcut icon" type="image/x-icon" href="{{ asset('bundles/kateglodefault/images/favicon.ico') }}"/>
    <link rel="icon" type="image/png" href="{{ asset('bundles/kateglodefault/images/favicon.png') }}"/>
    {% block stylesheets %}
        {% stylesheets
        '@KategloDefaultBundle/Resources/public/css/vendor/font.css'
        '@KategloDefaultBundle/Resources/public/css/vendor/bootstrap.min.css'
        '@KategloDefaultBundle/Resources/public/css/vendor/bootstrap-responsive.min.css'
        '@KategloDefaultBundle/Resources/public/css/main.css' output='bundles/kateglodefault/css/*' %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css" media="all"/>
        {% endstylesheets %}
    {% endblock %}
    {% block headscripts %}
        {% javascripts
        '@KategloDefaultBundle/Resources/public/js/vendor/html5.js'
        '@KategloDefaultBundle/Resources/public/js/vendor/modernizr-2.6.2-respond-1.1.0.min.js' output='bundles/kateglodefault/js/*' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% endblock %}
</head>
<body>
